	<text xml:lang="it" subtype="manuscript">
		<body>
			<div type="pagina" n="87">
				<pb n="87" facs="#P87Intera" />
				<!-- Inizio pagina -->
				<ab type="pericope" n="1" xml:id="Pag87Pericope1">
					<lb xml:id="P87R1" n="1" facs="#P87Riga1"/>
					Cosa avverrebbe domani se un governo
					<lb xml:id="P87R2" n="2" facs="#P87Riga2"/>
                    assoluto cadesse nelle mani di fana-
					<lb xml:id="P87R3" n="3" facs="#P87Riga3"/>
					tici, incapaci di discurete e di dubitare,
					<lb xml:id="P87R4" n="4" facs="#P87Riga4"/>
					esasperati dalle persecuzioni,pronti a 
					<lb xml:id="P87R5" n="5" facs="#P87Riga5"/>
					dare la vita come hanno già <unclear reason="illegible">sacri- 
					<lb xml:id="P87R6" n="6" facs="#P87Riga6"/>
					ficato</unclear> la propria personalità <pc type="interrogative" force= "strong">?</pc>
				</ab> 
				<ab type="pericope" n="2" xml:id="Pag87Pericope2">
				    <lb xml:id="P87R7" n="7" facs="#P87Riga7"/>
					<unclear reason="illegible"> chi anno </unclear> <gap reason="cancelled"/> moriva <gap reason="illegible"/> un
                    <lb xml:id="P87R8" n="8" facs="#P87Riga8"/>
					anno oggi, a quest' ora, <gap reason="illegible"/>
					<lb xml:id="P87R9" n="9" facs="#P87Riga9"/>
					mi diceva che secondo le ultime<gap reason="cancelled"/>
					<lb xml:id="P87R10" n="10" facs="#P87Riga10"/>
					<unclear reason="illegible"> notizie le </unclear> sue condizioni erano di-
					<lb xml:id="P87R11" n="11" facs="#P87Riga11"/>
					<unclear reason="illegible">sperate.</unclear>
                </ab>
				<ab type="pericope" n="3" xml:id="Pag87Pericope3">
				    <lb xml:id="P87R12" n="12" facs="#P87Riga12"/>
                    I fascisti fanno schifo, i <unclear reason= "illegible">Nazisti</unclear>
					<lb xml:id="P87R13" n="13" facs="#P87Riga13"/>
					orrore, i comunisti spavento.
				</ab>
				<ab type="pericope" n="4" xml:id="Pag87Pericope4">
                    <lb xml:id="P87R14" n="14" facs="#P87Riga14"/>
					Stamane andai a salutare i miei
					<lb xml:id="P87R15" n="15" facs="#P87Riga15"/>
					padroni di casa e poi lessi un ro-
					<lb xml:id="P87R16" n="16" facs="#P87Riga16"/>
					manzo di cui parlerò quando
					<lb xml:id="P87R17" n="17" facs="#P87Riga17"/>
					sarà finito,<gap reason="cancelled" quantity="3" unit="chars"/> nel pomeriggio
					<lb xml:id="P87R18" n="18" facs="#P87Riga18"/>
					mi avviai pigramente verso
					<lb xml:id="P87R19" n="19" facs="#P87Riga19"/>
					<abbr>S.</abbr> Antonio, senza immaginare
	            </ab>
			</div>

            <div type="pagina" n="88">
				<pb n="88" facs="#P88Intera"/>
				<!-- Inizio pagina -->
				<ab type="pericope" n="1" xml:id="Pag88Pericope1">
					<lb xml:id="P88R1" n="1" facs="#P88Riga1"/>
					che la giornata,cominciata in modo
					<lb xml:id="P88R2" n="2" facs="#P88Riga2"/>
					molto insulso, sarebbe finita av-
			        <lb xml:id="P88R3" n="3" facs="#P88Riga3"/>
					venturosamente.
				</ab> 
				<ab type="pericope" n="2" xml:id="Pag88Pericope2">
					Per la strada in-
					<lb xml:id="P88R4" n="4" facs="#P88Riga4"/>
					contro <persName> Maretta</persName> che mi dice:<q>vuoi
					<lb xml:id="P88R5" n="5" facs="#P88Riga5"/>
					fare un colpo<pc type="interrogative" force= "strong">?</pc></q>
				</ab>
				<ab type="pericope" n="3" xml:id="Pag88Pericope3">
					<del rend="overstrike">Ora par</del> <unclear reason="illegible"></unclear>Stavano
                    <lb xml:id="P88R6" n="6" facs="#P88Riga6"/>
					per partire 10 uomini per am-
					<lb xml:id="P88R7" n="7" facs="#P88Riga7"/>
					mazzare 50 <unclear reason="illegible">tedeschi. </unclear><del rend="overstrike">Na
					<lb xml:id="P88R8" n="8" facs="#P88Riga8"/>
					turalmente</del><gap reason="illegible" />
					<lb xml:id="P88R9" n="9" facs="#P88Riga9"/>
                    <unclear reason="illegible"> prendere il manoscritto</unclear><gap reason="illegible" />
					<lb xml:id="P88R10" n="10" facs="#P88Riga10"/>
					a mano.
				</ab>
				<ab type="pericope" n="4" xml:id="Pag88Pericope4" > 
					<said direct="false"><unclear reason="illegible">Naturalmente </unclear> <add place="margin"> <gap reason="illegible"/> </add>dissi
                    <lb xml:id="P88R11" n="11" facs="#P88Riga11"/>
					di si</said>, ma confesso che aveva
					<lb xml:id="P88R12" n="12" facs="#P88Riga11+2"/>
					una certa <unclear reason="illegible">preoccupazione né </unclear>
					<lb xml:id="P88R13" n="13" facs="#P88Riga13"/>
		             <gap reason="illegible"  extent="2 words"/> in <unclear reason="illegible">mente</unclear> che <name type="person">Maretta</name>
					 <lb xml:id="P88R14" n="14" facs="#P88Riga14"/>
					 esagerava come il solito.
				</ab> 
                <ab type="pericope" n="5" xml:id="Pag88Pericope5"> 
                     Abile
					 <lb xml:id="P88R15" n="15" facs="#P88Riga15"/>
					 gli dissi e partii per la base con
		            <lb xml:id="P88R16" n="16" facs="#P88Riga16"/>
					la speranza che la spedizione
					<lb xml:id="P88R17" n="17" facs="#P88Riga17"/>
					fosse diretta da <name type="person">Balestrieri</name>, che
					<lb xml:id="P88R18" n="18" facs="#P88Riga18"/>
					mi inspira una fiducia parti-
					<lb xml:id="P88R19" n="19" facs="#P88Riga19"/>
					colare.
				</ab> 
				<ab type="pericope" n="6" xml:id="Pag88Pericope6">
					Per la strada guardavo
					<lb xml:id="P88R20" n="20" facs="#P88Riga20"/>
					il sole luminoso e il cielo azzurro
					<lb xml:id="P88R21" n="21" facs="#P88Riga21"/>
					pensando: <said aloud="false">sarebbe un peccato morire
                    <lb xml:id="P88R22" n="22" facs="#P88Riga22"/>
				     <gap reason="cancelled" extent="1 word"/>in una così bella giornata</said>
				</ab>
            </div>
            <div type="pagina" n="158">
				<pb n="158" facs="#P158Intera"/>
				<!-- Inizio pagina -->
				<ab type="pericope" n="1" xml:id="Pag158Pericope1">
					<lb xml:id="P158R1" n="1" facs="#P158Riga1"/>
		             anti comunisti, eppure si sottomettevano
					 <lb xml:id="P158R2" n="2" facs="#P158Riga2"/>
					 ai commissari.
				 </ab>
				<ab type="pericope" n="2" xml:id="Pag158Pericope2"> 
					Esempio quello di
					 <lb xml:id="P158R3" n="3" facs="#P158Riga3" />
		             <persName >Fala</persName> che si è lamentato di<gap reason="cancelled" extent="1 words"/><abbr type="initial">P.</abbr>che
                     <lb xml:id="P158R4" n="4" facs="#P158Riga4"/>
					 gli toglie le sue prerogative.
				</ab>
				<ab type="pericope" n="3" xml:id="Pag158Pericope3"> 
					 <abbr type="initial">P.</abbr> da me 
					 <lb xml:id="P158R5" n="5" facs="#P158Riga5"/>
					 interrogato, mi ha risposto che <persName>Fala</persName>
					 <lb xml:id="P158R6" n="6" facs="#P158Riga6"/>
                     è un <unclear reason="illegible">  intrigante </unclear> e troppo severo con i
		             <lb xml:id="P158R7" n="7" facs="#P158Riga7"/>
					 soldati.
			   </ab>
				<ab type="pericope" n="4" xml:id="Pag158Pericope4">
					 Questa seconda accusa è 
		             <lb xml:id="P158R8" n="8" facs="#P158Riga8"/>
		             giusta.
				</ab>
				<ab type="pericope" n="5" xml:id="Pag158Pericope5">
				    Allora<gap reason="cancelled" extent="4 words"/>
		            <lb xml:id="P158R9" n="9" facs="#P158Riga9"/>
					l' ho promesso da facente funzioni
					<lb xml:id="P158R10" n="10" facs="#P158Riga10" />
					di commissario a commissario e <del rend="overstrike">l'ho
					<lb xml:id="P158R11" n="11" facs="#P158Riga11"/>
                    spedito R</del> lo manderò a <name type="place">Prali</name>
                    <lb xml:id="P158R12" n="12" facs="#P158Riga12"/>
					dove<add place="supralinear">era</add> trasferita la squadra
					<lb xml:id="P158R13" n="13" facs="#P158Riga13" />
					degli Fivert.
				</ab>
				<ab type="pericope" n="6" xml:id="Pag158Pericope6">
		            <lb xml:id="P158R14" n="14" facs="#P158Riga14"/>
					Ora scenderanno a <name type="place">Billari</name> per sentire
					<lb xml:id="P158R15" n="15" facs="#P158Riga15"/>
					la relazione di ieri fra <del rend="overstrike">Giorgio e
                    <lb xml:id="P158R16" n="16" facs="#P158Riga16"/>
					<persName>Nello </persName>,<persName > Giorgio</persName> e gli altri.
					</del>
				</ab>
				<ab type="pericope" n="7" xml:id="Pag158Pericope7">
					<lb xml:id="P158R17" n="17" facs="#P158Riga17"/>
					Questo diario è molto mal scritto,
					<lb xml:id="P158R18" n="18" facs="#P158Riga18"/>
					ma ho pochi momenti al giorno
					<lb xml:id="P158R19" n="19" facs="#P158Riga19"/>
					da dedicargli e molte cose<gap reason="illegible"/>
					<lb xml:id="P158R20" n="20" facs="#P158Riga20"/>
					da dire.
				</ab>
